    "url": "https://github.com/samchon/nestia/issues"
  },
  "homepage": "https://nestia.io",
  "files": [
    "lib",
    "package.json",
    "README.md"
  ],
  "devDependencies": {
    "rimraf": "^6.0.1",
    "ts-patch": "^3.3.0",
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "@nestia/fetcher": "^6.0.1",
    "typia": "^9.0.1"
  }
}